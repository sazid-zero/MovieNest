{
  "expo": {
    "name": "MovieNest",
    "slug": "MovieNest",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/logo.png",
    "scheme": "movienest",
    "backgroundColor": "#030014",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#030014",
        "foregroundImage": "./assets/icons/logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.sazidzero.MovieNest"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/icons/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/splash-transparent.png",
          "imageWidth": 1,
          "resizeMode": "contain",
          "backgroundColor": "#030014",
          "dark": {
            "image": "./assets/icons/splash-transparent.png",
            "backgroundColor": "#030014"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0084f1d3-2b59-4edc-b2ff-ea5f7865ca31"
      }
    }
  }
}
